{
  "name": "@nanostores/router",
  "version": "0.15.1",
  "description": "A tiny (712 bytes) router for Nano Stores state manager",
  "keywords": [
    "nano",
    "router",
    "react",
    "preact",
    "vue",
    "svelte",
    "store"
  ],
  "scripts": {
    "test:lint": "eslint .",
    "test:coverage": "c8 pnpm bnt",
    "test:types": "check-dts",
    "test:size": "size-limit",
    "test": "pnpm run /^test:/"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "nanostores/router",
  "sideEffects": false,
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/ai"
    }
  ],
  "peerDependencies": {
    "nanostores": "^0.9.0 || ^0.10.0 || ^0.11.0"
  },
  "devDependencies": {
    "@logux/eslint-config": "^53.4.1",
    "@size-limit/preset-small-lib": "^11.1.6",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.7",
    "better-node-test": "^0.7.1",
    "c8": "^10.1.2",
    "check-dts": "^0.8.2",
    "clean-publish": "^5.0.0",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "nanostores": "^0.11.3",
    "size-limit": "^11.1.6",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "c8": {
    "exclude": [
      "**/*.test.*",
      "test/*"
    ],
    "lines": 100,
    "check-coverage": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "skip-full": true,
    "clean": true
  },
  "size-limit": [
    {
      "name": "Router",
      "import": {
        "./index.js": "{ createRouter }"
      },
      "limit": "712 B"
    }
  ],
  "clean-publish": {
    "cleanDocs": true
  }
}
